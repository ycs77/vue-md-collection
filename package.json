{
  "name": "vite-plugin-collection",
  "version": "0.1.0",
  "description": "The file based markdown collection for Vue 3 apps using Vite.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup -c",
    "watch": "cross-env NODE_ENV=development rollup -c -w",
    "build": "cross-env NODE_ENV=production rollup -c",
    "compress": "uglifyjs dist/jquery.bsmodal.js -o dist/jquery.bsmodal.min.js -c -m --comments /^!/",
    "release": "npm run build && npm run compress"
  },
